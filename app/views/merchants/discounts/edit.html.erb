<%= form_with url: merchant_discount_path(@discount.merchant.id, @discount.id), method: :patch, local: true do |f| %>
<%= f.label :percentage %>
<%= f.number_field :percentage, min: 1, max: 100, value: @discount.percentage %><br>

<%= f.label :quantity %>
<%= f.number_field :quantity, min: 1, value: @discount.quantity %><br>

  <%= f.submit 'Update Bulk Discount' %>
<% end %>
